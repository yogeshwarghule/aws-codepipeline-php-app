{
    "executionRoleArn": "arn:aws:iam::316026101470:role/ecsTaskExecutionRole",  
    "containerDefinitions": [
        {
            "name": "web1",
            "image": "316026101470.dkr.ecr.ap-south-1.amazonaws.com/php-application-2022:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "task-defi"
}
